{
	"manifest_version": 2,
	"name": "gSuggest",
	"description": "Improvements to your friend's search experience",
	"version": "1.0.1",
	"permissions": [
		"activeTab"
	],
	"icons": { "128": "icon.png"},
	"content_scripts": [
		{
		"matches": ["https://www.google.com/*", "http://www.google.com/*"],
		"js": ["resources/jquery-1.11.3.min.js","searchSuggest.js"]
		}
	]
}
